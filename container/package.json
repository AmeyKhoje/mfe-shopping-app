    "babel-loader": "^8.2.5",
    "concurrently": "^8.2.1",
    "core-js": "^3.25.5",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
